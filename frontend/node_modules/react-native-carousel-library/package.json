{
  "name": "react-native-carousel-library",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "react-native",
    "carousel",
    "ios",
    "android",
    "mobile",
    "horizontal carousel",
    "animated horizontal carousel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aexol-studio/react-native-ts-carousel-library.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "prepare": "bob build"
  },
  "dependencies": {
    "validator": "^13.7.0"
  },
  "peerDependencies": {
    "react": "17.x",
    "react-native": ">=0.60"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.34",
    "@types/react-native": "^0.66.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/validator": "^13.7.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-builder-bob": "^0.18.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.x.x"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "main": "lib/commonjs/index.ts",
  "react-native": "src/index.ts",
  "module": "lib/module/index.ts",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
